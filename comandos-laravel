-- Soft Delete
  Agregar una migraciÃ³n
    public function up()
    {
        Schema::table('black_list', function (Blueprint $table) {
            $table->softDeletes();  //add this line
        });
    }
    public function down()
    {
        Schema::table('black_list', function (Blueprint $table) {
            $table->dropSoftDeletes(); //add this line
