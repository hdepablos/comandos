-- Ejecutar servidor
  php artisan serve
  
-- Optimizar
  php artisan clear-compiled && php artisan view:clear && php artisan route:clear && php artisan
 config:cache && php artisan cache:clear && php artisan optimize && php artisan route:cache && php artisan config:clear

-- Crear controlador Api
  php artisan make:controller /Api/CheckBcraController
  
-- Crear modelo
  php artisan make:model Http/Models/Padron
